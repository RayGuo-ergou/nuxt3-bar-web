{
  "name": "chin-website",
  "version": "0.1.0",
  "description": "a Nuxt 3 starter template or boilerplate with a lot of useful features. Nuxt3 + Tailwindcss 3",
  "repository": {
    "type": "git",
    "url": "https://github.com/RayGuo-ergou/nuxt3-bar-web.git"
  },
  "author": "RayGuo-ergou",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "serve": "serve dist/",
    "postinstall": "nuxt prepare && husky install",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lintfix": "eslint --fix --ext \".ts,.vue\" --ignore-path .eslintignore .",
    "prepare": "husky install",
    "clean": "rm -rf .nuxt dist .output",
    "generate:locales": "node tools/translator.js ./locales en.yml",
    "vercel-build": "prisma generate && prisma migrate deploy && nuxt build",
    "sync:content": "bash ./scripts/syncMDwithDB.bash",
    "database:reset": "prisma migrate reset --force",
    "database:deploy": "prisma generate && prisma migrate deploy"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@headlessui/vue": "^1.7.13",
    "@iconify/json": "^2.2.64",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@nuxt/content": "2.6.0",
    "@nuxt/test-utils-edge": "3.5.0-28066801.7f0fe2fb",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/supabase": "^0.3.5",
    "@nuxtjs/tailwindcss": "^6.6.8",
    "@pinia/nuxt": "^0.4.10",
    "@prisma/client": "^4.14.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@trpc/client": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "@types/node": "^20.1.4",
    "@vueuse/components": "^10.1.2",
    "@vueuse/nuxt": "^10.1.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "flowbite": "^1.6.5",
    "h3": "^1.6.5",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.2.2",
    "mavon-editor": "3.0.0",
    "nuxt": "3.4.3",
    "pinia": "^2.0.36",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "sass": "1.62.1",
    "sass-loader": "^13.2.2",
    "serve": "^14.2.0",
    "superjson": "^1.12.3",
    "tailwindcss-animate": "^1.0.5",
    "translate": "^2.0.2",
    "trpc-nuxt": "^0.10.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "unplugin-icons": "^0.16.1",
    "vite": "4.3.5",
    "vitest": "^0.31.0",
    "vue": "^3.3.2",
    "vue-recaptcha": "^2.0.3",
    "vue-toastification": "2.0.0-rc.5",
    "vue-tsc": "^1.6.5",
    "webpack": "^5.82.1",
    "zod": "^3.21.4"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html}": [
      "pnpm lintfix"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "tailwindcss"
      ]
    }
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
